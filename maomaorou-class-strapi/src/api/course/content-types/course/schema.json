{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "課程",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text",
      "default": "預設課程介紹",
      "required": true
    },
    "goal": {
      "type": "text",
      "default": "預設課程目標",
      "required": true
    },
    "renewPrice": {
      "default": 100,
      "required": true,
      "type": "integer"
    },
    "renewDurationDay": {
      "type": "integer",
      "required": true,
      "default": 30
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter",
      "mappedBy": "course"
    },
    "title": {
      "type": "string",
      "required": true,
      "default": "課程名稱",
      "unique": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "courses"
    },
    "sequence": {
      "type": "integer",
      "required": true,
      "default": 0,
      "min": 0,
      "unique": true
    },
    "firstPrice": {
      "type": "integer",
      "required": true,
      "default": 100
    },
    "firstDurationDay": {
      "type": "integer",
      "required": true,
      "default": 30
    }
  }
}
